  "criticalEdgeId": "E13",
  "startOptions": ["Mudrek", "Arselan"],
  "scenarioPresets": {
    "A": {
      "label": "What if Mudrek joins ServerAdmins?",
      "description": "Mudrek gets added to ServerAdmins group → he gains AdminTo FileServer and DC01, opening a direct 3-hop path to the DC.",
      "mutations": [
        { "type": "addEdge", "source": "Mudrek", "relation": "MemberOf", "target": "ServerAdmins", "weight": 3 }
      ]
    },
    "B": {
      "label": "What if Arselan has GenericAll on DomainAdmins?",
      "description": "Arselan acquires GenericAll ACL on DomainAdmins → he can add himself to the DA group and own the domain without needing Mutaz.",
      "mutations": [
        { "type": "addEdge", "source": "Arselan", "relation": "GenericAll", "target": "DomainAdmins", "weight": 9 }
      ]
    },
    "C": {
      "label": "What if Workstation01 has a session of Mutaz?",
      "description": "Mutaz logs into Workstation01 → any user with AdminTo on that workstation can steal its credentials and reach DomainAdmins.",
      "mutations": [
        { "type": "addEdge", "source": "Workstation01", "relation": "HasSession", "target": "Mutaz", "weight": 6 }
      ]
    },
    "D": {
      "label": "What if we revoke Mutaz → GenericAll → DomainAdmins?",
      "description": "Remove the critical GenericAll ACE — this is the most dangerous escalation path. Expected: ≥60% risk reduction.",
      "mutations": [
        { "type": "removeEdge", "edgeId": "E13" }
      ]
    },
    "E": {
      "label": "What if we disable Mutaz entirely?",
      "description": "Disable the over-privileged service account and remove all its edges. Eliminates every path that chains through it.",
      "mutations": [
        { "type": "removeNode", "nodeId": "Mutaz" }
      ]
    },
    "F": {
      "label": "What if we remove Arselan → WriteDACL → Mutaz?",
      "description": "Remove Arselan's WriteDACL on Mutaz — a low-impact fix that blocks one intermediate link but leaves other paths open.",
      "mutations": [
        { "type": "removeEdge", "edgeId": "E14" }
      ]
    }
  }
}
